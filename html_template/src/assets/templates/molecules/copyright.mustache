{{#mol-copyright}}
    <div class="home__copyright__content">
        <div class="home__copyright__content__info">
            {{#text}}
                <p>{{text}}</p>
            {{/text}}
        </div>


        <div class="home__copyright__content__social">
            {{#social-url}}
                <ul class="social-icons">
                    <li><a href="{{{social-url}}}" class="social-icon"> <i class="fa fa-twitter"></i></a></li>
                    {{#social-url}}
                        <li><a href="{{{social-url}}}" class="social-icon"> <i class="fa fa-linkedin"></i></a></li>
                    {{/social-url}}
                </ul>
            {{/social-url}}
        </div>
    </div>
{{/mol-copyright}}